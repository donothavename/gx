  if i<=245 then
    画布.drawLine(0,0.375*w-4, 0,0.375*w, 画笔)
  elseif i<280 or (i>320 and i<565) then
    画布.drawLine(0,0.375*w-4, 0,0.375*w, 画笔2)
  elseif i>=565 and i<=600 then
    画布.drawLine(0,0.375*w-3, 0,0.375*w-1, 画笔3)
  end 
  画布.rotate(0.5,0,0)
end
end))
