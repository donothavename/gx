function 提示(t)
  local tsbj={
    LinearLayout,
    {
      CardView,
      radius="4dp",
      Elevation="8dp",
      layout_margin="8dp",
      CardBackgroundColor=0xff202124,
      {
        LinearLayout,
        padding="16dp",
        layout_width=w-16/32*geth(toolbar),
        {
          TextView,
          Text=tostring(t),
          textSize="14sp",
          paddingLeft="8dp",
          paddingRight="8dp",
          textColor=0xffffffff,
        },
      },
    },
  }
  if toast then
    toast.cancel()
    toast=nil
  end
  toast=Toast.makeText(activity,t,Toast.LENGTH_SHORT).setGravity(Gravity.BOTTOM|Gravity.CENTER, 0, 0).setView(loadlayout(tsbj))
  toast.show()
end
